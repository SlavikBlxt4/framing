{
  "expo": {
    "entryPoint": "node_modules/expo/AppEntry.js",
    "name": "App-Fotografos",
    "slug": "App-Fotografos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2ffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.framing.AppFotografos"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#f2ffff"
      },
      "package": "com.framing.AppFotografos"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "493681e1-31a4-4117-8328-1f71f463a6d5"
      }
    },
    "owner": "slavik_blxt"
  }
}
